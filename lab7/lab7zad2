/*2. Даны три целых числа: A, B, C. Проверить истинность
высказывания: «Справедливо двойное неравенство A<B<C».*/
#include <iostream>
using namespace std;

int main()
{
 int A,B,C;
 cout <<"Enter A,B and C: \n";
 cin >>A>>B>>C;
 if(A<B&&B<C) cout<<"A<B<C true";
 else cout<<"A<B<C false";
return 0;
}
