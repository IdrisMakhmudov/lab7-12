/* 3. Дано целое число N (N > 0). Найти квадрат данного числа, используя
для его вычисления следующую формулу: N^2 = 1 + 3 + 5 + . . . + (2·N − 1).
После добавления к сумме каждого слагаемого выводить текущее значение
суммы */
#include <iostream>
using namespace std;

int main()
{
int N,m,n;
cin>>N;
m=0;
if(N>0) 
{
    for(double i=1; i<=N; i++) 
    {
     n=(2*i)-1;
     m+=n;
     cout<<m<<"\n";
    }
}
    else 
    cout<<"Invalid values";
}

