/* 1. Дано вещественное число — цена 1 кг конфет. Вывести стоимость
0.1, 0.2, . . . , 1 кг конфет. */
#include <iostream>
using namespace std;

int main()
{
double a;
cin>>a;
if(a>=0) 
{
    for(double i=0.1; i<=1; i+=0.1) 
    {
        cout<<" price of "<<i<<" kg sweets is "<<a*i<<"\n";
    }
}
    else 
    cout<<"Invalid values";
}

